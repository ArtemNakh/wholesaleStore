@model IEnumerable<wholesaleStore.Core.Models.Enterprice>

@{
    ViewData["Title"] = "Enterprices";
}

<h1>Enterprices</h1>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Date Created</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var enterprice in Model)
        {
            <tr>
                <td>@enterprice.Title</td>
                <td>@enterprice.Email</td>
                <td>@enterprice.Phone</td>
                <td>@enterprice.DateCreate</td>
                <td>
                    <a href="@Url.Action("Details", "Enterprice", new { id = enterprice.Id })" class="btn btn-info">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>
